MAAA,GAJA,eAIA,KAJA,EAIA;AACA,aAAA,MAAA,CAAA,QAAA,CAAA,cAAA,EAAA,KAAA;AACA;AANA;AAJA,IAdA;AA4BA,EAAA,OAAA,EAAA;AACA,IAAA,eADA,6BACA;AACA,aAAA;AACA,QAAA,UAAA,EAAA,EADA;AAEA,QAAA,cAAA,EAAA,EAFA;AAGA,QAAA,WAAA,EAAA,EAHA;AAIA,QAAA,QAAA,EAAA,CAJA;AAKA,QAAA,MAAA,EAAA,CALA;AAMA,QAAA,SAAA,EAAA;AANA,OAAA;AAQA,KAVA;AAWA,IAAA,eAXA,2BAWA,EAXA,EAWA;AACA,UAAA,QAAA,GAAA,KAAA,SAAA,CAAA,SAAA,CAAA,MAAA,CACA,UAAA,GAAA;AAAA,eAAA,GAAA,CAAA,cAAA,KAAA,EAAA;AAAA,OADA,CAAA;AAGA,WAAA,MAAA,CAAA,QAAA,CAAA,aAAA,EAAA,QAAA,CAAA,CAAA,CAAA;AACA,WAAA,WAAA,CAAA,WAAA,GAAA,KAAA,SAAA,CAAA,QAAA,CAAA,WAAA;AACA,MAAA,OAAA,CAAA,GAAA,CACA,yBADA,EAEA,KAAA,SAAA,CAAA,QAAA,CAAA,WAFA;AAIA,KArBA;AAsBA,IAAA,cAtBA,4BAsBA;AACA,UAAA,UAAA,GAAA,KAAA,OAAA,CAAA,gBAAA,CAAA,UAAA;AACA,UAAA,cAAA,GAAA,KAAA,WAAA,CAAA,cAAA;AACA,UAAA,WAAA,GAAA,KAAA,WAAA,CAAA,WAAA;AACA,UAAA,QAAA,GAAA,QAAA,CAAA,KAAA,WAAA,CAAA,QAAA,EAAA,EAAA,CAAA;AACA,UAAA,MAAA,GAAA,QAAA,CAAA,KAAA,WAAA,CAAA,MAAA,EAAA,EAAA,CAAA;AACA,UAAA,QAAA,GAAA,CAAA;AACA,WAAA,MAAA,CAAA,QAAA,CAAA,kBAAA,EAAA;AACA,QAAA,UAAA,EAAA,UADA;AAEA,QAAA,cAAA,EAAA,cAFA;AAGA,QAAA,WAAA,EAAA,WAHA;AAIA,QAAA,QAAA,EAAA,QAJA;AAKA,QAAA,MAAA,EAAA;AALA,OAAA;AAOA,KApCA;AAsCA,IAAA,KAtCA,mBAsCA;AACA,WAAA,MAAA,GAAA,KAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,MAAA;AACA;AAzCA;AA5BA,CAAA","sourcesContent":["<template>\n  <v-card outlined class=\"mx-auto\" max-width=\"1300\">\n    <v-form v-model=\"valid\">\n      <v-container>\n        <v-row>\n          <!-- Productos -->\n          <!-- Agregar Productos -->\n          <v-col cols=\"12\" md=\"1\">\n            <v-tooltip bottom>\n              <template v-slot:activator=\"{ on, attrs }\">\n                <v-btn\n                  v-bind=\"attrs\"\n                  v-on=\"on\"\n                  class=\"mx-2\"\n                  fab\n                  dark\n                  color=\"indigo\"\n                  @click=\"modal = !modal\"\n                >\n                  <v-icon dark> mdi-plus </v-icon>\n                </v-btn>\n              </template>\n              <span>Agregar nuevo Producto</span>\n            </v-tooltip>\n          </v-col>\n          <!-- Seleccionar Productos -->\n          <v-col cols=\"12\" md=\"4\">\n            <v-autocomplete\n              v-model=\"add_Compras.CodigoProducto\"\n              :items=\"vProducto.productos\"\n              :item-text=\"\n                (item) => `[${item.CodigoProducto}] - ${item.Descripcion}`\n              \"\n              item-value=\"CodigoProducto\"\n              label=\"Seleccionar producto\"\n              outlined\n              @change=\"(id) => obtenerProducto(id)\"\n            ></v-autocomplete>\n          </v-col>\n\n          <v-col cols=\"12\" md=\"2\">\n            <v-text-field\n              v-model=\"add_Compras.Cantidad\"\n              label=\"Cantidad\"\n              required\n              outlined\n            ></v-text-field>\n          </v-col>\n\n          <v-col cols=\"12\" md=\"3\">\n            <v-text-field\n              v-model=\"add_Compras.Precio\"\n              label=\"Precio\"\n              required\n              outlined\n            ></v-text-field>\n          </v-col>\n          <v-col cols=\"12\" md=\"1\">\n            <v-tooltip bottom>\n              <template v-slot:activator=\"{ on, attrs }\">\n                <v-btn\n                  v-bind=\"attrs\"\n                  v-on=\"on\"\n                  class=\"mx-2\"\n                  fab\n                  dark\n                  color=\"green\"\n                  @click=\"agregarCompras\"\n                >\n                  <v-icon dark> mdi-cart-check </v-icon>\n                </v-btn>\n              </template>\n              <span>Agregar Compra</span>\n            </v-tooltip>\n          </v-col>\n        </v-row>\n      </v-container>\n    </v-form>\n    <!-- Dialogo Producto -->\n    <v-dialog v-model=\"modal\" max-width=\"1000px\">\n      <FormularioProducto />\n    </v-dialog>\n  </v-card>\n</template>\n\n<script>\nimport FormularioProducto from \"../../producto/FormularioProducto\";\nimport { mapGetters, mapState } from \"vuex\";\nexport default {\n  data() {\n    return {\n      add_Compras: this.detalle_compras(),\n    };\n  },\n  mounted() {\n    let header = { \"auth-token\": this.$store.state.token };\n    let configuracion = { headers: header };\n    this.$store.dispatch(\"getProductos\", configuracion);\n  },\n  components: {\n    FormularioProducto,\n  },\n  computed: {\n    ...mapState([\"vProducto\", \"compras\"]),\n    ...mapGetters([\"addItemToCompras\"]),\n\n    modal: {\n      get() {\n        return this.$store.getters.getModal;\n      },\n      set(value) {\n        this.$store.dispatch(\"switchDialog\", value);\n      },\n    },\n  },\n\n  methods: {\n    detalle_compras() {\n      return {\n        NroFactura: \"\",\n        CodigoProducto: \"\",\n        Descripcion: \"\",\n        Cantidad: 1,\n        Precio: 0,\n        SubtTotal: 0,\n      };\n    },\n    obtenerProducto(id) {\n      let producto = this.vProducto.productos.filter(\n        (pro) => pro.CodigoProducto === id\n      );\n      this.$store.dispatch(\"getProducto\", producto[0]);\n      this.add_Compras.Descripcion = this.vProducto.Producto.Descripcion;\n      console.log(\n        \"Producto seleccionado: \",\n        this.vProducto.Producto.Descripcion\n      );\n    },\n    agregarCompras() {\n      const NroFactura = this.compras.cabecera_compras.NroFactura;\n      const CodigoProducto = this.add_Compras.CodigoProducto;\n      const Descripcion = this.add_Compras.Descripcion;\n      const Cantidad = parseInt(this.add_Compras.Cantidad, 10);\n      const Precio = parseInt(this.add_Compras.Precio, 10);\n      const SubTotal = 0;\n      this.$store.dispatch(\"addItemToCompras\", {\n        NroFactura,\n        CodigoProducto,\n        Descripcion,\n        Cantidad,\n        Precio,\n      });\n    },\n\n    close() {\n      this.dialog = false;\n      console.log(\"paso\");\n    },\n  },\n};\n</script>>"],"sourceRoot":"src/views/compras/compra"}]}